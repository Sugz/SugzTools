<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <!-- Header Arrows -->
    <PathGeometry x:Key="RolloutCollapsedGeo"
                  Figures="M0,0 L4,3 0,6 Z" />
    <PathGeometry x:Key="RolloutCollapsedInvertedGeo"
                  Figures="M4,0 L0,3 4,6 Z" />
    <PathGeometry x:Key="RolloutExpandedGeo"
                  Figures="M0,0 L6,0 3,4 Z" />
    <PathGeometry x:Key="RolloutDragDropGeo"
                  Figures="M0,0 H1 V1 H0 M2,0 H3 V1 H2 M4,0 H5 V1 H4 M0,2 H1 V3 H0 M2,2 H3 V3 H2 M4,2 H5 V3 H4 M0,4 H1 V5 H0 M2,4 H3 V5 H2 M4,4 H5 V5 H4 Z" />
    
    
    <!--CheckMark-->
    <PathGeometry x:Key="CheckMarkGeo"
                  Figures="M1,1 2,3 8,0 2,6 0,3 Z" />
    
    
    <!--NumericUpDown Arrows-->
    <PathGeometry x:Key="NumericUpDownUpGeo"
                  Figures="M0,3 L5,3 2.5,0 Z" />
    <PathGeometry x:Key="NumericUpDownDownGeo"
                  Figures="M0,0 L5,0 2.5,3 Z" />

    
    <!--ScrollBars Arrows-->
    <PathGeometry x:Key="ScrollBarUpGeo"
                  Figures="M0,5 7,5 3.5,0 Z" />
    <PathGeometry x:Key="ScrollBarDownGeo"
                  Figures="M0,0 7,0 3.5,5 Z" />
    <PathGeometry x:Key="ScrollBarLeftGeo"
                  Figures="M5,0 5,7 0,3.5 Z" />
    <PathGeometry x:Key="ScrollBarRightGeo"
                  Figures="M0,0 0,7 5,3.5 Z" />

</ResourceDictionary>