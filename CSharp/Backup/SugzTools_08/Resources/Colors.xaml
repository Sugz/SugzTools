<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--3ds Max 2017 Brushes-->
    <SolidColorBrush x:Key="MaxBackground" Color="#FF444444" />

    <SolidColorBrush x:Key="MaxRollout" Color="#FF515151" />
    <SolidColorBrush x:Key="MaxRolloutBorder" Color="#FF3E3E3E" />
    <SolidColorBrush x:Key="MaxRolloutArrow" Color="#FFB7B7B7" />
    <SolidColorBrush x:Key="MaxRolloutIcon" Color="#FF979797" />

    <SolidColorBrush x:Key="MaxButton" Color="#FF646464" />
    <SolidColorBrush x:Key="MaxButtonBorder" Color="#FF494949" />
    <SolidColorBrush x:Key="MaxButtonMouseOver" Color="#FF383838" />
    <SolidColorBrush x:Key="MaxCaretBrush" Color="#FF9B9B9B" />

    <SolidColorBrush x:Key="MaxText" Color="#FFE1E1E1" />
    <SolidColorBrush x:Key="MaxTitle" Color="#FFFFFFFF" />

    <SolidColorBrush x:Key="MaxBlue" Color="#FF5F8AC1" />
    <SolidColorBrush x:Key="MaxBlueMouseOver" Color="#FF496A93" />

    <SolidColorBrush x:Key="MaxFocusBorder" Color="#FFD0D0D0" />

</ResourceDictionary>